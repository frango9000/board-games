{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "architect": {
        "clean": {
          "builder": "@nxrocks/nx-spring-boot:clean",
          "options": {
            "root": "apps/api",
            "args": ["-U", "-B"]
          }
        },
        "test": {
          "builder": "@nxrocks/nx-spring-boot:clean",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "apps/api/target",
            "root": "apps/api",
            "args": ["verify", "-U", "-B"]
          }
        },
        "build": {
          "builder": "@nxrocks/nx-spring-boot:clean",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "apps/api/target",
            "root": "apps/api",
            "args": ["verify", "-U", "-B", "-DskipTests", "-DskipITs"]
          }
        },
        "package": {
          "builder": "@nxrocks/nx-spring-boot:clean",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "apps/api/target",
            "root": "apps/api",
            "args": ["package", "-U", "-B", "-DskipTests", "-DskipITs", "-P frontend"]
          }
        },
        "deploy": {
          "builder": "@nxrocks/nx-spring-boot:clean",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "apps/api/target",
            "root": "apps/api",
            "args": ["deploy", "-U", "-B", "-DskipTests", "-DskipITs", "-P frontend,herokudev"]
          }
        },
        "pact": {
          "builder": "@nxrocks/nx-spring-boot:clean",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "apps/api/target",
            "root": "apps/api",
            "args": [
              "verify",
              "-U",
              "-B",
              "-D it.test=\"dev.kurama.api.pact.**\"",
              "-Dtest=\"dev.kurama.api.null.**\"",
              "-DfailIfNoTests=false"
            ]
          }
        },
        "run": {
          "builder": "@nxrocks/nx-spring-boot:run",
          "options": {
            "root": "apps/api"
          }
        },
        "serve": {
          "builder": "@nxrocks/nx-spring-boot:serve",
          "options": {
            "root": "apps/api",
            "args": ["-DskipTests"]
          }
        },
        "buildJar": {
          "builder": "@nxrocks/nx-spring-boot:buildJar",
          "options": {
            "root": "apps/api"
          }
        },
        "buildWar": {
          "builder": "@nxrocks/nx-spring-boot:buildWar",
          "options": {
            "root": "apps/api"
          }
        },
        "buildImage": {
          "builder": "@nxrocks/nx-spring-boot:buildImage",
          "options": {
            "root": "apps/api"
          }
        },
        "buildInfo": {
          "builder": "@nxrocks/nx-spring-boot:buildInfo",
          "options": {
            "root": "apps/api"
          }
        }
      },
      "tags": []
    },
    "app": {
      "projectType": "application",
      "root": "apps/app",
      "sourceRoot": "apps/app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "allowedCommonJsDependencies": ["sockjs-client"],
            "outputPath": "dist/apps/app",
            "index": "apps/app/src/index.html",
            "main": "apps/app/src/main.ts",
            "polyfills": "apps/app/src/polyfills.ts",
            "tsConfig": "apps/app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/app/src/favicon.ico", "apps/app/src/assets"],
            "styles": ["./node_modules/ngx-toastr/toastr.css", "apps/app/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "685kb",
                  "maximumError": "700kb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/app/src/environments/environment.ts",
                  "with": "apps/app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app:build",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            },
            "development": {
              "browserTarget": "app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/app/src/**/*.ts", "apps/app/src/**/*.html"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/app"],
          "options": {
            "jestConfig": "apps/app/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "app-e2e": {
      "root": "apps/app-e2e",
      "sourceRoot": "apps/app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/app-e2e/cypress.json",
            "tsConfig": "apps/app-e2e/tsconfig.e2e.json",
            "devServerTarget": "app:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/app-e2e/**/*.{js,ts}"]
          },
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": [],
      "implicitDependencies": ["app"]
    },
    "consumer-pact": {
      "projectType": "library",
      "root": "libs/consumer-pact",
      "sourceRoot": "libs/consumer-pact/src",
      "prefix": "pact",
      "architect": {
        "pact": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["apps/api/target/test-classes/pact"],
          "options": {
            "jestConfig": "libs/consumer-pact/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/consumer-pact/src/**/*.ts"]
          }
        }
      },
      "tags": []
    },
    "domain": {
      "projectType": "library",
      "root": "libs/domain",
      "sourceRoot": "libs/domain/src",
      "prefix": "app",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/domain"],
          "options": {
            "jestConfig": "libs/domain/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/domain/src/**/*.ts", "libs/domain/src/**/*.html"]
          },
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": []
    },
    "hal-form-client": {
      "projectType": "library",
      "root": "libs/hal-form-client",
      "sourceRoot": "libs/hal-form-client/src",
      "prefix": "app",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/hal-form-client"],
          "options": {
            "jestConfig": "libs/hal-form-client/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/hal-form-client/src/**/*.ts", "libs/hal-form-client/src/**/*.html"]
          },
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": []
    },
    "ui-core": {
      "projectType": "library",
      "root": "libs/ui/core",
      "sourceRoot": "libs/ui/core/src",
      "prefix": "app",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/core"],
          "options": {
            "jestConfig": "libs/ui/core/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/core/src/**/*.ts", "libs/ui/core/src/**/*.html"]
          }
        }
      },
      "tags": []
    },
    "ui-feature-authorization": {
      "projectType": "library",
      "root": "libs/ui/feature/authorization",
      "sourceRoot": "libs/ui/feature/authorization/src",
      "prefix": "app",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/feature/authorization"],
          "options": {
            "jestConfig": "libs/ui/feature/authorization/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui/feature/authorization/src/**/*.ts",
              "libs/ui/feature/authorization/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "ui-feature-home": {
      "projectType": "library",
      "root": "libs/ui/feature/home",
      "sourceRoot": "libs/ui/feature/home/src",
      "prefix": "app",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/feature/home"],
          "options": {
            "jestConfig": "libs/ui/feature/home/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/feature/home/src/**/*.ts", "libs/ui/feature/home/src/**/*.html"]
          }
        }
      },
      "tags": []
    },
    "ui-shared": {
      "projectType": "library",
      "root": "libs/ui/shared",
      "sourceRoot": "libs/ui/shared/src",
      "prefix": "app",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui/shared"],
          "options": {
            "jestConfig": "libs/ui/shared/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/shared/src/**/*.ts", "libs/ui/shared/src/**/*.html"]
          }
        }
      },
      "tags": []
    }
  }
}
