{
  "version": 1,
  "cli": {
    "packageManager": "yarn",
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "chess-app",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "style": "scss",
        "spec": true,
        "linter": "eslint",
        "unitTestRunner": "jest",
        "e2eTestRunner": "cypress"
      },
      "library": {
        "style": "scss",
        "spec": true,
        "linter": "eslint",
        "unitTestRunner": "jest"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "chess-app": {
      "projectType": "application",
      "root": "apps/chess-app",
      "sourceRoot": "apps/chess-app/src",
      "prefix": "chess-lite",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/chess-app",
            "index": "apps/chess-app/src/index.html",
            "main": "apps/chess-app/src/main.ts",
            "polyfills": "apps/chess-app/src/polyfills.ts",
            "tsConfig": "apps/chess-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/chess-app/src/favicon.ico", "apps/chess-app/src/assets"],
            "styles": ["./node_modules/ngx-toastr/toastr.css", "apps/chess-app/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/chess-app/src/environments/environment.ts",
                  "with": "apps/chess-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "chess-app:build",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "chess-app:build:production"
            },
            "development": {
              "browserTarget": "chess-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "chess-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/chess-app/src/**/*.ts", "apps/chess-app/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/chess-app"],
          "options": {
            "jestConfig": "apps/chess-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "chess-app-e2e": {
      "root": "apps/chess-app-e2e",
      "sourceRoot": "apps/chess-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/chess-app-e2e/cypress.json",
            "tsConfig": "apps/chess-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "chess-app:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "chess-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/chess-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "chess-backend": {
      "root": "apps/chess-backend",
      "sourceRoot": "apps/chess-backend/src",
      "projectType": "application",
      "architect": {
        "clean": {
          "builder": "@nxrocks/nx-spring-boot:clean",
          "options": {
            "root": "apps/chess-backend"
          }
        },
        "test": {
          "builder": "@nxrocks/nx-spring-boot:clean",
          "options": {
            "root": "apps/chess-backend",
            "args": ["verify", "-U", "-B"]
          }
        },
        "build": {
          "builder": "@nxrocks/nx-spring-boot:clean",
          "options": {
            "root": "apps/chess-backend",
            "args": ["verify", "-U", "-B", "-DskipTests", "-DskipITs"]
          }
        },
        "run": {
          "builder": "@nxrocks/nx-spring-boot:run",
          "options": {
            "root": "apps/chess-backend"
          }
        },
        "serve": {
          "builder": "@nxrocks/nx-spring-boot:serve",
          "options": {
            "root": "apps/chess-backend"
          }
        },
        "buildJar": {
          "builder": "@nxrocks/nx-spring-boot:buildJar",
          "options": {
            "root": "apps/chess-backend"
          }
        },
        "buildWar": {
          "builder": "@nxrocks/nx-spring-boot:buildWar",
          "options": {
            "root": "apps/chess-backend"
          }
        },
        "buildImage": {
          "builder": "@nxrocks/nx-spring-boot:buildImage",
          "options": {
            "root": "apps/chess-backend"
          }
        },
        "buildInfo": {
          "builder": "@nxrocks/nx-spring-boot:buildInfo",
          "options": {
            "root": "apps/chess-backend"
          }
        }
      }
    },
    "domain": {
      "projectType": "library",
      "root": "libs/domain",
      "sourceRoot": "libs/domain/src",
      "prefix": "chess-lite",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/domain"],
          "options": {
            "jestConfig": "libs/domain/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/domain/src/**/*.ts", "libs/domain/src/**/*.html"]
          }
        }
      }
    },
    "hal-form-client": {
      "projectType": "library",
      "root": "libs/hal-form-client",
      "sourceRoot": "libs/hal-form-client/src",
      "prefix": "chess-lite",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/hal-form-client"],
          "options": {
            "jestConfig": "libs/hal-form-client/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/hal-form-client/src/**/*.ts", "libs/hal-form-client/src/**/*.html"]
          }
        }
      }
    }
  }
}
