{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/api/src",
  "projectType": "application",
  "targets": {
    "clean": {
      "executor": "@nxrocks/nx-spring-boot:clean",
      "options": {
        "root": "apps/api"
      }
    },
    "build": {
      "executor": "@nxrocks/nx-spring-boot:clean",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "apps/api/target",
        "root": "apps/api",
        "args": [
          "compile"
        ]
      }
    },
    "test": {
      "executor": "@nxrocks/nx-spring-boot:clean",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "apps/api/target",
        "root": "apps/api",
        "args": [
          "verify",
          "-P pact"
        ]
      }
    },
    "pact": {
      "executor": "@nxrocks/nx-spring-boot:clean",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "apps/api/target",
        "root": "apps/api",
        "args": [
          "verify",
          "-P pact",
          "-D it.test=\"**/*PactIT.java\"",
          "-Dtest=\"dev.kurama.api.null.**\"",
          "-Dsurefire.failIfNoSpecifiedTests=false",
          "-DskipUTs"
        ]
      }
    },
    "e2e": {
      "executor": "@nxrocks/nx-spring-boot:clean",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "coverage/apps/app-e2e",
        "root": "apps/api",
        "args": [
          "verify",
          "-P frontend,e2e",
          "-Dtest=\"dev.kurama.api.null.**\"",
          "-Dsurefire.failIfNoSpecifiedTests=false",
          "-DskipUTs"
        ]
      }
    },
    "package": {
      "executor": "@nxrocks/nx-spring-boot:clean",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "apps/api/target",
        "root": "apps/api",
        "args": [
          "package",
          "-DskipTests",
          "-DskipITs",
          "-P frontend"
        ]
      }
    },
    "deploy": {
      "executor": "@nxrocks/nx-spring-boot:clean",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "apps/api/target",
        "root": "apps/api"
      },
      "configurations": {
        "production": {
          "args": [
            "deploy",
            "-DskipTests",
            "-DskipITs",
            "-P frontend",
            "-P heroku-production"
          ]
        },
        "development": {
          "args": [
            "deploy",
            "-DskipTests",
            "-DskipITs",
            "-P frontend",
            "-P heroku-development"
          ]
        },
        "feature": {
          "args": [
            "deploy",
            "-DskipTests",
            "-DskipITs",
            "-P frontend",
            "-P heroku-feature"
          ]
        }
      },
      "defaultConfiguration": "development"
    },
    "run": {
      "executor": "@nxrocks/nx-spring-boot:run",
      "options": {
        "root": "apps/api"
      }
    },
    "serve": {
      "executor": "@nxrocks/nx-spring-boot:serve",
      "options": {
        "root": "apps/api"
      }
    },
    "format": {
      "executor": "@nxrocks/nx-spring-boot:format",
      "options": {
        "root": "apps/api"
      }
    },
    "format-check": {
      "executor": "@nxrocks/nx-spring-boot:format-check",
      "options": {
        "root": "apps/api"
      }
    },
    "buildJar": {
      "executor": "@nxrocks/nx-spring-boot:buildJar",
      "options": {
        "root": "apps/api"
      }
    },
    "buildWar": {
      "executor": "@nxrocks/nx-spring-boot:buildWar",
      "options": {
        "root": "apps/api"
      }
    },
    "buildImage": {
      "executor": "@nxrocks/nx-spring-boot:buildImage",
      "options": {
        "root": "apps/api"
      }
    },
    "buildInfo": {
      "executor": "@nxrocks/nx-spring-boot:buildInfo",
      "options": {
        "root": "apps/api"
      }
    }
  },
  "tags": [],
  "implicitDependencies": [
    "consumer-pact"
  ]
}
