{
  "extends": ["../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts"],
      "extends": ["plugin:github/typescript", "plugin:@nrwl/nx/angular"],
      "rules": {
        "@typescript-eslint/no-namespace": "off",
        "@typescript-eslint/no-non-null-assertion": "off",
        "@nrwl/nx/enforce-module-boundaries": "off"
      }
    }
  ]
}
