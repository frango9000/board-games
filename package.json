{
  "name": "app",
  "version": "0.0.1-SNAPSHOT",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start:frontend": "nx run-many --target=serve --projects=app",
    "start:backend": "nx run-many --target=serve --projects=api",
    "analyze": "webpack-bundle-analyzer dist/apps/app/stats.json",
    "analyze:bundle": "npm run build:frontend:stats && npm run analyze",
    "build": "nx run-many --target=build --all",
    "build:frontend": "nx run-many --target=build --projects=app",
    "build:frontend:prod": "nx run-many --target=build --projects=app --skip-nx-cache --output-path=apps/api/target/classes/static/app --deploy-url=/app/ --base-href=/app/",
    "build:frontend:stats": "nx run-many --target=build --projects=app --statsJson",
    "build:backend": "nx run-many --target=build --projects=api",
    "package": "nx run-many --target=package --projects=api",
    "test": "nx run-many --target=test --all",
    "test:frontend": "nx run-many --target=test --projects=app",
    "test:backend": "nx run-many --target=test --projects=api",
    "lint": "nx workspace-lint && nx run-many --target=lint --all",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write --all",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "migrations": "nx migrate --run-migrations",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "docker:dev:up": "docker compose up -d",
    "docker:dev:stop": "docker compose stop",
    "docker:dev:down": "docker compose down -v",
    "validate": "npm run lint && nx format:check && npm run test && npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.3",
    "@angular/cdk": "12.2.3",
    "@angular/common": "12.2.3",
    "@angular/compiler": "12.2.3",
    "@angular/core": "12.2.3",
    "@angular/forms": "12.2.3",
    "@angular/material": "12.2.3",
    "@angular/platform-browser": "12.2.3",
    "@angular/platform-browser-dynamic": "12.2.3",
    "@angular/router": "12.2.3",
    "@ngneat/until-destroy": "8.1.4",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@nrwl/angular": "12.8.0",
    "angular-animations": "0.11.0",
    "jwt-decode": "3.1.2",
    "ngx-toastr": "14.1.0",
    "rxjs": "7.3.0",
    "tslib": "2.3.1",
    "url-template": "2.0.8",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.3",
    "@angular-devkit/schematics": "12.2.3",
    "@angular-devkit/schematics-cli": "12.2.4",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "12.2.3",
    "@angular/compiler-cli": "12.2.3",
    "@angular/language-service": "12.2.3",
    "@nrwl/cli": "12.8.0",
    "@nrwl/cypress": "12.8.0",
    "@nrwl/devkit": "12.8.0",
    "@nrwl/eslint-plugin-nx": "12.8.0",
    "@nrwl/jest": "12.8.0",
    "@nrwl/linter": "12.8.0",
    "@nrwl/node": "12.8.0",
    "@nrwl/tao": "12.8.0",
    "@nrwl/workspace": "12.8.0",
    "@nx-tools/nx-docker": "1.0.5",
    "@nxrocks/nx-spring-boot": "3.0.0",
    "@schematics/angular": "12.2.3",
    "@schematics/schematics": "0.1102.12",
    "@types/jest": "27.0.1",
    "@types/node": "16.7.6",
    "@types/url-template": "2.0.28",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "cypress": "8.3.1",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.11.3",
    "jest": "27.1.0",
    "jest-preset-angular": "9.0.7",
    "prettier": "2.3.2",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "typescript": "4.3.5",
    "webpack-bundle-analyzer": "4.4.2"
  }
}
